<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" version="3.1">
  <display-name>onlinetest</display-name>
  <welcome-file-list>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>
  <servlet-mapping>
    <servlet-name>login</servlet-name>
    <url-pattern>/login</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>login</servlet-name>
    <servlet-class>com.onlinetest.servlets.LoginServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>register</servlet-name>
    <url-pattern>/register</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>register</servlet-name>
    <servlet-class>com.onlinetest.servlets.RegisterServlet</servlet-class>
  </servlet>
   <servlet-mapping>
    <servlet-name>afterLogin</servlet-name>
    <url-pattern>/afterLogin</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>afterLogin</servlet-name>
    <servlet-class>com.onlinetest.servlets.AfterLoginServlet</servlet-class>
  </servlet>
   <servlet-mapping>
    <servlet-name>getQuestions</servlet-name>
    <url-pattern>/getQuestions</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>getQuestions</servlet-name>
    <servlet-class>com.onlinetest.servlets.GetQuestionsServlet</servlet-class>
  </servlet>
   <servlet-mapping>
    <servlet-name>submitTest</servlet-name>
    <url-pattern>/submitTest</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>submitTest</servlet-name>
    <servlet-class>com.onlinetest.servlets.SubmitTestServlet</servlet-class>
  </servlet>
</web-app>